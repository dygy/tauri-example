{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "category": "Business",
    "copyright": "Lol",
    "targets": "all",
    "externalBin": [],
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "Desktop",
  "version": "0.1.0",
  "identifier": "com.example",
  "plugins": {},
  "app": {
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "Desktop",
        "width": 1280
      }
    ],
    "security": {
      "dangerousDisableAssetCspModification": true,
      "devCsp": null,
      "capabilities": [],
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$AUDIO",
          "$CACHE",
          "$CONFIG",
          "$DATA",
          "$LOCALDATA",
          "$DESKTOP",
          "$DOCUMENT",
          "$DOWNLOAD",
          "$EXE",
          "$FONT",
          "$HOME",
          "$PICTURE",
          "$PUBLIC",
          "$RUNTIME",
          "$TEMPLATE",
          "$VIDEO",
          "$RESOURCE",
          "$APP",
          "$LOG",
          "$TEMP",
          "$APPCONFIG",
          "$APPDATA",
          "$APPLOCALDATA",
          "$APPCACHE",
          "$APPLOG"
        ]
      },
      "csp": null
    }
  }
}
